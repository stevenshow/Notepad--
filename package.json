{
	"name": "notedpad--",
	"version": "0.0.0",
	"license": "MIT",
	"main": "main/index.js",
	"author": {
		"name": "Steve & Trevor"
	},
	"description": "Starter using Vite 2 + React + Typescript + Electron for fast prototyping.",
	"keywords": [
		"vite",
		"react",
		"electron",
		"typescript"
	],
	"productName": "vite-reactts-electron-starter",
	"scripts": {
		"dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
		"dev:vite": "vite",
		"dev:electron": "yarn build:electron && electron .",
		"build": "yarn build:vite && yarn build:electron",
		"build:vite": "vite build",
		"build:electron": "tsc -p electron",
		"dist": "yarn build && electron-builder",
		"pack": "yarn build && electron-builder --dir",
		"clean": "rimraf dist main src/out",
		"type-check": "tsc",
		"lint": "eslint . --ext js,ts,tsx",
		"lint:fix": "eslint . --ext js,ts,tsx --fix",
		"format": "prettier --write .",
		"prepare": "husky install",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"new-component": "plop"
	},
	"dependencies": {
		"@vitejs/plugin-react": "^2.0.0",
		"clsx": "^1.2.1",
		"electron-is-dev": "^2.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"slate": "^0.87.0",
		"slate-react": "^0.87.0",
		"vite": "^3.0.0",
		"vite-tsconfig-paths": "^3.6.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@storybook/addon-actions": "^6.5.14",
		"@storybook/addon-essentials": "^6.5.14",
		"@storybook/addon-interactions": "^6.5.14",
		"@storybook/addon-links": "^6.5.14",
		"@storybook/builder-vite": "^0.2.5",
		"@storybook/react": "^6.5.14",
		"@storybook/testing-library": "^0.0.13",
		"@trivago/prettier-plugin-sort-imports": "^4.0.0",
		"@types/node": "^18.0.15",
		"@types/react": "^18.0.6",
		"@types/react-dom": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "5.33.0",
		"@typescript-eslint/parser": "5.33.0",
		"autoprefixer": "^10.4.4",
		"babel-loader": "^8.3.0",
		"concurrently": "^7.6.0",
		"electron": "^22.0.0",
		"electron-builder": "^22.14.13",
		"eslint": "8.21.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.5.2",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "^7.30.1",
		"husky": "^8.0.0",
		"lint-staged": "^13.1.0",
		"plop": "^3.1.1",
		"postcss": "^8.4.12",
		"prettier": "^2.8.0",
		"prettier-plugin-tailwindcss": "^0.2.0",
		"pretty-quick": "^3.1.3",
		"tailwindcss": "^3.1.8",
		"typescript": "^4.6.4"
	},
	"build": {
		"asar": true,
		"files": [
			"main",
			"src/out"
		],
		"directories": {
			"buildResources": "resources"
		}
	},
	"lint-staged": {
		"*.{ts,tsx,js}": "eslint"
	}
}
